APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://besttrend-sy.com
FRONTEND_URL=hhttps://besttrend-sy.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# PostgreSQL Configuration for Neon
DB_CONNECTION=pgsql

# Database connection string (for cloud databases like Neon)
DATABASE_URL="pgsql://username:password@host:5432/database?sslmode=require"

# Fallback connection parameters
DB_CONNECTION=pgsql
DB_HOST=your-database-host
DB_PORT=5432
DB_DATABASE=your-database-name
DB_USERNAME=your-database-username
DB_PASSWORD=your-database-password
DB_SSLMODE=require

# Additional Neon-specific settings
NEON_PROJECT_ID=your-neon-project-id


BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
